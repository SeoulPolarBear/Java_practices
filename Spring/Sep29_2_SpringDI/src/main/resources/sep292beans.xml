<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="d" class="com.polarbear.sep292.dog.Dog"></bean>
	
	
	<bean id="d2" class="com.polarbear.sep292.dog.Dog">
		<property name="name" value="개님"></property>
		<property name="age" value="40"></property>
	</bean>
	<bean id="d3" class="com.polarbear.sep292.dog.Dog">
		<constructor-arg value="개느님"></constructor-arg>
		<constructor-arg value="99"></constructor-arg>
	</bean>
	<bean id="b1" class="com.polarbear.sep292.book.Book">
		<property name="name" value="기차길은 위험해"></property>
		<property name="price" value="15000"></property>
	</bean>
	<bean id="b2" class="com.polarbear.sep292.book.Book">
		<constructor-arg value="바다에서 조개구이"></constructor-arg>
		<constructor-arg value="10000"></constructor-arg>
	</bean>
</beans>
